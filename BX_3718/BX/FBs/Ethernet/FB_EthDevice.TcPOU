    <Action Name="A_Receive" Id="{44b562d5-7166-4ed6-8902-d98806e6705c}">
      <Implementation>
        <ST><![CDATA[	(*--------------------- Get received message strings --------------------------------*)
	REPEAT
		fbRx.RemoveHead( );(* Fetch string from rx fifo *)
		IF fbRx.bOk THEN(* Success *)
	
		(* TODO: Implement receive data handler *)
			sTemData := fbRx.sGet; 		
			
		END_IF
	UNTIL NOT fbRx.bOk
	END_REPEAT
]]></ST>
      </Implementation>
    </Action>
    <Method Name="Send" Id="{6d0fdf11-2b9c-45c0-94d0-44e8e8ce1a4f}">
      <Declaration><![CDATA[METHOD Send : BOOL
VAR_INPUT
	sTxData		: T_MaxString;	(* Data to send *)
END_VAR
VAR
	RTRIG_SendCommand			: R_TRIG;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[		fbTx.AddTail( sPut := sTxData );(* Put string to the tx fifo *)
		]]></ST>
      </Implementation>
    </Method>

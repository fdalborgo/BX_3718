  <POU Name="FB_EthDevice" Id="{c11f7280-27ca-4f3d-865b-a554dd255326}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK FB_EthDevice
VAR_INPUT
	bEnable		: BOOL 			:= FALSE;				(* TRUE => enable client data exchange, FALSE => disable *)
	sSrvNetID	: T_AmsNetID	:= '';					(* TwinCAT TCP/IP server network address *)
	sRemoteHost	: T_IPv4Addr 	:= '127.0.0.1';			(* Server network address *)
	nRemotePort	: UDINT 		:= 200;					(* Server port number *)
	bLogEnable	: BOOL 			:= FALSE;				(* TRUE => Enable log message output, FALSE => Disable *)
	sLogPrefix	: STRING 		:= 'CLIENT::';			(* Log message description string (allows the identification of log message source) *)
END_VAR
VAR_OUTPUT
	sData						: T_MaxString;			(* Output received Data *)
	bConnected					: BOOL := FALSE;		(* TCP/IP connection state *)	
END_VAR
VAR
	fbConnection				: FB_ClientConnection;	(* Client connection/data exchange control function block *)
	fbTx 						: FB_FrameFifo;			(* Tx fifo *)
	fbRx 						: FB_FrameFifo;			(* Rx fifo *)
	
	sLogMessage					: T_MaxString;
	sTemData					: T_MaxString;			(* Output received Data *)
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[IF bConnected := fbConnection.bConnected THEN
	A_Receive();
END_IF

A_Call();

IF NOT fbConnection.RxBusy THEN
	sData := sTemData;
END_IF]]></ST>
    </Implementation>
